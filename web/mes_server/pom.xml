            <!-- Maven 自带的中央仓库使用的Id为central 如果其他的仓库声明也是用该Id 就会覆盖中央仓库的配置 -->
            <id>nexus</id>
            <name>nexus</name>
            <url>http://172.26.132.9:8081/repository/maven-releases/</url>
